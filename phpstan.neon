parameters:
	checkMissingIterableValueType: false
	inferPrivatePropertyTypeFromConstructor: true
	level: max
	paths:
		- src
		- tests
	excludes_analyse:
		- src/ObjectFieldsPermissions/DependencyInjection/*
		- src/ObjectFieldsPermissions/ObjectFieldsPermissionsBundle.php
	ignoreErrors:
		- "#is not allowed to extend#"
	tmpDir: %currentWorkingDirectory%/.build/phpstan
